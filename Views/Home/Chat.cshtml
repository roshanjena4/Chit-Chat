@model List<ChatApplication.Models.User>
<div class="chat-container">
    <aside class="sidebar">
        <div class="sidebar-header">
            <h2 class="sidebar-title">CHIT CHAT</h2>
        </div>
        <input type="hidden" id="currentUserId" value="@Context.Session.GetString("UserId")" />

        <nav class="contact-list">  
            <ul>
                @foreach (var user in Model)
                {
                    <li class="contact-item active-chat" onclick="selectUser('@user.Id','@user.Email','@user.Name')">
                        <img src="https://placehold.co/40x40/7F56D9/FFFFFF?text=@user.Name.Substring(0,2)" alt="avi_chovatiya" class="avatar" onerror="this.src='https://placehold.co/40x40/cccccc/000000?text=??'; this.onerror=null;">
                        <span class="contact-name">@user.Name</span>
                    </li>
                   
                }
            </ul>
        </nav>
        <div class="current-user-profile">
            <img src="https://placehold.co/40x40/6941C6/FFFFFF?text=@Context.Session.GetString("Name")?.Substring(0,2)" alt="Richa_denials" class="avatar" onerror="this.src='https://placehold.co/40x40/cccccc/000000?text=??'; this.onerror=null;">
            <span class="contact-name">@Context.Session.GetString("Name")</span>

            <form method="post" asp-action="Logout">
            <button id="logoutButton" class="logout-button" aria-label="Logout" type="submit">
    

                <svg width="20" height="19" viewBox="0 0 9 9" fill="none" xmlns="http://www.w3.org/2000/svg">
                    <path d="M5.1546 0.210132L4.22869 0.214496L4.2505 4.84407L5.17642 4.83971L5.1546 0.210132ZM7.39542 1.20421L6.74112 1.86471C7.47537 2.44922 7.94721 3.34978 7.95199 4.36366C7.96043 6.15531 6.5182 7.61119 4.72655 7.61964C2.9349 7.62808 1.47902 6.18585 1.47058 4.3942C1.4658 3.38033 1.92913 2.47536 2.65318 1.87935L1.99734 1.22965C1.1028 1.99776 0.538682 3.13006 0.544659 4.39857C0.555502 6.69947 2.43001 8.5564 4.73091 8.54555C7.03182 8.53471 8.88874 6.6602 8.8779 4.3593C8.87192 3.09079 8.29716 1.96386 7.39542 1.20421Z" fill="#F3F3F3" />
                </svg>

            </button>
            </form>
        </div>
    </aside>

    
    <main class="chat-area">
        <div class="chat-welcome" id="chatWelcomeMessage" >
            <div class="robot-icon">
                <svg width="70" height="71" viewBox="0 0 70 71" fill="none" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
                    <rect x="0.279526" y="0.914795" width="99.4445" height="99.4445" fill="url(#pattern0_1_112)" />
                    <defs>
                        <pattern id="pattern0_1_112" patternContentUnits="objectBoundingBox" width="1" height="1">
                            <use xlink:href="#image0_1_112" transform="scale(0.002)" />
                        </pattern>
                        <image id="image0_1_112" width="500" height="500" preserveAspectRatio="none" xlink:href="data:image/png;base64," />
                    </defs>
                </svg>
            </div>
            <p>Welcome, <span class="welcome-username">@Context.Session.GetString("Name")</span>!</p>
            <p>Please select a chat to start messaging.</p>
        </div>

        <div class="active-chat-view" id="activeChatView" style="display:none;">
            <header class="active-chat-header">
                <div class="chat-user-info">
                    <img src="https://placehold.co/40x40/344054/FFFFFF?text=AV" alt="avi_chovatiya67" class="avatar" id="chatHeaderAvatar" onerror="this.src='https://placehold.co/40x40/cccccc/000000?text=??'; this.onerror=null;">
                    <span class="chat-user-name" id="chatHeaderName">avi_chovatiya67</span>
                </div>
                <button class="chat-power-button" aria-label="Options" onclick="showChatWelcomeMessage()">
                    <i class="fas fa-power-off"></i>
                </button>
            </header>

           

            <footer class="chat-input-area ">
                <button class="emoji-button" aria-label="Add emoji">
                    <i class="far fa-smile"></i>
                </button>
                <input type="text" id="messageInput" placeholder="Type your message here">
                <button id="btnSend" class="send-button" aria-label="Send message">
                    <svg width="9" height="8" viewBox="0 0 9 8" fill="none" xmlns="http://www.w3.org/2000/svg">
                        <path d="M0.95413 7.81009L8.91999 4.30029L0.921405 0.86572L0.930329 3.56632L6.63827 4.31104L0.937601 5.10952L0.95413 7.81009Z" fill="#F3F3F3" />
                    </svg>

                </button>
            </footer>
        </div>
    </main>
</div>

